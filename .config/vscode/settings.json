    "editor.fontSize": 15,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.fontSize": 19,
    "workbench.activityBar.visible": false,
    "git.decorations.enabled": false,
